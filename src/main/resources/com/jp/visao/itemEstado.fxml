<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.shape.Line?>


<BorderPane fx:id="root" xmlns="http://javafx.com/javafx/20.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.jp.visao.itemEstado">
   <center>
      <HBox fx:id="hboxEstado" alignment="CENTER" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" spacing="50.0">
         <children>
            <TextField fx:id="tfNome" onKeyTyped="#nameChange" />
         </children>
         <padding>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
         </padding>
      </HBox>
   </center>
   <right>
      <Button cancelButton="true" mnemonicParsing="false" onAction="#removeState" BorderPane.alignment="CENTER">
         <graphic>
            <AnchorPane>
               <children>
                  <Line endX="21.0" endY="20.0" layoutY="1.0" startY="2.0" />
                  <Line endX="-20.0" endY="20.0" layoutX="20.0" layoutY="1.0" startY="2.0" />
               </children>
            </AnchorPane>
         </graphic>
      </Button>
   </right>
   <padding>
      <Insets left="10.0" right="10.0" />
   </padding>
</BorderPane>
